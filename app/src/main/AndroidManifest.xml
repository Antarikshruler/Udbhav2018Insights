<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.msritudbhav.udbhav2017insights">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Utils.Udbhav2017Insights"
        android:allowBackup="true"
        android:icon="@mipmap/u"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/u_circle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.AddRegistrationActivity" />
        <activity
            android:name=".Activities.EmailLoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.HistoryActivity"></activity>
    </application>

</manifest>